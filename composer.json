
{
    "name": "tysweezy/decipher-api-php-client",
    "description": "PHP Client for the Decipher REST API 2.0",
    "keywords": [
        "Decipher",
        "package",
        "REST",
        "API"
    ],
    "homepage": "https://github.com/tysweezy/decipher-api-php-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Tyler Souza",
            "email": "tsouza.sf@gmail.com",
            "homepage": "http://tysweezy.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Decipher\\Api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Decipher\\Api\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}